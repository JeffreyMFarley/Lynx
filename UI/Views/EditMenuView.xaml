<Menu x:Class="Lynx.UI.Views.EditMenuView"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <MenuItem Header="Paste New Entity" 
              Command="{Binding PasteNewEntity}" 
              InputGestureText="Ctrl+E" />
    <MenuItem Header="Paste Entity Update" 
              InputGestureText="Shift+Ctrl+E"
              IsEnabled="False"
              />
    <Separator />
    <MenuItem Header="Paste New Link" 
              InputGestureText="Ctrl+L" 
              IsEnabled="False"
              />
    <MenuItem Header="Paste Link Update" 
              InputGestureText="Shift+Ctrl+L" 
              IsEnabled="False"
              />
</Menu>
